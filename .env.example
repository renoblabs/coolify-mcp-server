# Copy this to .env and fill in your actual values

# Coolify Configuration
COOLIFY_API_TOKEN=your_coolify_api_token_here
COOLIFY_BASE_URL=http://your-dev-box-ip:8000
COOLIFY_TUNNEL_URL=https://cloud.therink.io
USE_TUNNEL=false
BASE_DOMAIN=therink.io

# Cloudflare Configuration  
CLOUDFLARE_API_TOKEN=your_cf_api_token_here
CLOUDFLARE_ZONE_ID=your_zone_id_here
CLOUDFLARE_TUNNEL_ID=your_tunnel_id_here

# MCP Server (HTTP/SSE)
MCP_AUTH_TOKEN=your_mcp_auth_token_here
MCP_PORT=8765
MCP_HOST=0.0.0.0
ALLOWED_ORIGINS=*